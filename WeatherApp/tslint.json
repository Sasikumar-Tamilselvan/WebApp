{
  "extends": "../tslint.json",
  "rules": {
      "directive-selector": [
          true,
          "attribute",
          "app",
          "camelCase"
      ],
      "component-selector": [
          true,
          "element",
          "app",
          "kebab-case"
      ],
      "no-empty-interface": false,
      "no-bitwise": false,
      "forin": false
  }
}
